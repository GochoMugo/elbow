{
  "name": "elbow",
  "version": "0.0.0",
  "description": "An easy way to test REST API responses with Mocha",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -D -q src --out-dir .",
    "pretest": "npm run build",
    "test": "grunt test",
    "test-coverage": "istanbul cover _mocha --report lcovonly -- -R spec test/test.*.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GochoMugo/elbow.git"
  },
  "keywords": [
    "REST",
    "API",
    "schema",
    "mocha"
  ],
  "author": "GochoMugo <mugo@forfuture.co.ke>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GochoMugo/elbow/issues"
  },
  "homepage": "https://github.com/GochoMugo/elbow#readme",
  "bin": {
    "elbow": "bin/elbow"
  },
  "dependencies": {
    "cli-output": "^0.1.0",
    "commander": "^2.8.1",
    "debug": "^2.2.0",
    "jayschema": "^0.3.1",
    "lodash": "^3.10.1",
    "superagent": "^1.3.0",
    "should": "^7.1.0"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "body-parser": "^1.13.3",
    "coveralls": "^2.11.3",
    "express": "^4.12.4",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-eslint": "^17.0.0",
    "grunt-mocha-test": "^0.12.7",
    "istanbul": "^0.3.17",
    "load-grunt-tasks": "^3.2.0",
    "mocha-lcov-reporter": "^0.0.2"
  }
}
