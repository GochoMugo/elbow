{
  "name": "elbow",
  "version": "0.5.0",
  "description": "An easy way to test REST API responses with Mocha",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -D -q src --out-dir .",
    "pretest": "npm run build",
    "test": "grunt test",
    "test-coverage": "istanbul cover _mocha --report lcovonly -- -R spec test/test.*.js",
    "prepublish": "npm run build",
    "clean": "rm -rf Gruntfile.js lib/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GochoMugo/elbow.git"
  },
  "keywords": [
    "REST",
    "API",
    "schema",
    "mocha"
  ],
  "author": "GochoMugo <mugo@forfuture.co.ke>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GochoMugo/elbow/issues"
  },
  "homepage": "https://github.com/GochoMugo/elbow#readme",
  "bin": {
    "elbow": "bin/elbow"
  },
  "dependencies": {
    "cli-output": "^0.1.0",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "jayschema": "^0.3.1",
    "lodash": "^4.3.0",
    "should": "^8.2.2",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "body-parser": "^1.14.2",
    "coveralls": "^2.11.6",
    "express": "^4.13.4",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-eslint": "^17.0.0",
    "grunt-mocha-test": "^0.12.7",
    "istanbul": "^0.4.2",
    "load-grunt-tasks": "^3.4.0",
    "mocha-lcov-reporter": "^1.0.0"
  }
}
