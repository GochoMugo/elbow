{
  "name": "elbow",
  "version": "1.0.0",
  "description": "An easy way to test REST API responses with Mocha",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "babel -D -q src --out-dir .",
    "pretest": "npm run build",
    "test": "grunt test",
    "test-coverage": "istanbul cover _mocha --report lcovonly -- -R spec test/test.*.js",
    "prepublish": "npm run clean && npm run build",
    "clean": "rm -rf Gruntfile.js lib/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GochoMugo/elbow.git"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "REST",
    "API",
    "schema",
    "mocha"
  ],
  "author": "GochoMugo <mugo@forfuture.co.ke>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GochoMugo/elbow/issues"
  },
  "homepage": "https://github.com/GochoMugo/elbow#readme",
  "bin": {
    "elbow": "bin/elbow"
  },
  "dependencies": {
    "ajv": "^5.1.4",
    "cli-output": "^1.0.0",
    "commander": "^2.9.0",
    "debug": "^2.6.8",
    "depd": "^1.1.1",
    "lodash": "^4.17.4",
    "should": "^11.2.1",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "coveralls": "^2.13.1",
    "express": "^4.15.3",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-eslint": "^20.0.0",
    "grunt-mocha-test": "^0.13.2",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.9.1",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
